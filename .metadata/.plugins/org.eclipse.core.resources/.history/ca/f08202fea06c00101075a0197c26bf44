package com.rahul.ecommerce.productservice;

import java.util.Arrays;

public class Test {

	public static void main(String[] args) {
		int[] arr = {0,1,0,1,1,0,1,0,0};
		int left=0;
		int right=arr.length-1;
			
			while(left < right)
			{
				arr[left]=0;
				arr[right]=1;
			    left++;
			    right--;
			}
		}
	System.out.println(Arrays.toString(arr));
}
